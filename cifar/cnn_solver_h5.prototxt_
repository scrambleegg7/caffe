train_net: "cnn_train.prototxt_"
test_net: "cnn_test.prototxt_"

test_iter: 100
test_interval: 1000

base_lr: 0.0006
momentum: 0.0
weight_decay: 0.001

lr_policy: "inv"
gamma: 0.0001
power: 0.75

display: 100

max_iter: 50000

snapshot: 10000
snapshot_prefix: "cnn_lmdb_snapshot"
solver_mode: CPU

type: "RMSProp"
rms_decay: 0.98
